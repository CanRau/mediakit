{
  "name": "@solid-mediakit/auth",
  "description": "An authentication utility library for Solid.",
  "version": "1.0.3",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && rollup -c && node scripts/postbuild",
    "clean": "pnpm clean:dist && rm -rf ./node_modules .turbo .solid",
    "lint": "eslint . --fix --ext .ts,.tsx,.js,.jsx",
    "clean:dist": "rm -rf dist compiler client.* index.* types.* utils.* unplugin.*",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "files": [
    "client.*",
    "index.*",
    "utils.*",
    "types.*",
    "src/**/*",
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "solid": "./index.js",
      "default": "./index.js"
    },
    "./client": {
      "types": "./client.d.ts",
      "solid": "./client.js",
      "default": "./client.js"
    },
    "./types": "./types.d.ts",
    "./utils": {
      "types": "./utils.d.ts",
      "default": "./utils.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@auth/core": "0.15.0",
    "@types/node": "^18.7.14",
    "@typescript-eslint/parser": "^5.44.0",
    "solid-js": "^1.8.14",
    "@solidjs/meta": "^0.29.3",
    "@solidjs/router": "^0.11.3",
    "@solidjs/start": "^0.5.2",
    "typescript": "^4.8.2",
    "vinxi": "^0.2.1"
  },
  "dependencies": {
    "unplugin": "^1.3.1",
    "@solid-mediakit/shared": "workspace:*"
  },
  "peerDependencies": {
    "@solidjs/meta": "^0.29.3",
    "@solidjs/router": "^0.11.3",
    "@solidjs/start": "^0.5.2",
    "typescript": "^4.8.2",
    "@auth/core": "0.15.0",
    "solid-js": "^1.8.14",
    "vinxi": "^0.2.1"
  },
  "engines": {
    "node": ">=16"
  }
}
