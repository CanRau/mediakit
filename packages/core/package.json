{
  "name": "@solid-mediakit/core",
  "description": "A set of utilities to use with your Solid apps.",
  "version": "1.0.0",
  "scripts": {
    "build": "rm -rf dist && tsup --config ../../tsup.config.js",
    "clean": "rm -rf ./node_modules dist .turbo .solid",
    "dev": "rm -rf dist && tsup --config ../../tsup.config.js --watch",
    "lint": "eslint . --fix --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "exports": {
    "worker": {
      "solid": "./dist/server.jsx",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/server.js"
      },
      "require": "./dist/server.cjs"
    },
    "browser": {
      "solid": {
        "development": "./dist/dev.jsx",
        "import": "./dist/index.jsx"
      },
      "development": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/dev.js"
        },
        "require": "./dist/dev.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.cjs"
    },
    "deno": {
      "solid": "./dist/server.jsx",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/server.js"
      },
      "require": "./dist/server.cjs"
    },
    "node": {
      "solid": "./dist/server.jsx",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/server.js"
      },
      "require": "./dist/server.cjs"
    },
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      },
      "require": "./dist/dev.cjs"
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "main": "./dist/server.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "browser": {
    "./dist/server.js": "./dist/index.js",
    "./dist/server.cjs": "./dist/index.cjs"
  },
  "dependencies": {
    "chrome-aws-lambda": "7.0.0",
    "puppeteer-core": "13.1.2",
    "twemoji": "13.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.7.14",
    "@typescript-eslint/parser": "^5.44.0",
    "solid-js": "^1.5.3",
    "typescript": "^4.8.2",
    "solid-start": "^0.2.0",
    "@types/puppeteer-core": "5.4.0"
  },
  "peerDependencies": {
    "solid-js": "^1.5.3",
    "solid-start": "^0.2.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16"
  },
  "typesVersions": {},
  "module": "./dist/server.js"
}
